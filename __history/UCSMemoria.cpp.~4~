#pragma package(smart_init)
CSMemoria::CSMemoria(){
	mem = new Nodo[MAX];
	for (int i = 0; i < MAX - 1; i++ ) {
		mem[i].link = i + 1;
	}
    mem[MAX - 1].link = NULO;
	libre = 0;
}

